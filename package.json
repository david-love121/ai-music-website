{
  "name": "ai-music-website",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "bunx vite dev",
    "build": "bunx vite build",
    "build:gh-pages": "BASE_PATH=/${npm_package_name} bunx vite build",
    "gen:manifest": "bash scripts/gen-manifest.sh",
    "preview": "bunx vite preview",
    "prepare": "bunx svelte-kit sync || echo ''",
    "check": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
  "@sveltejs/adapter-auto": "^6.0.0",
  "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.0.0",
    "vite": "^7.0.4"
  },
  "dependencies": {
  }
}
